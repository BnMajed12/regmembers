<?xml version="1.0" encoding="utf-8"?>
<com.example.regdemo.CheckableRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/onajina"
        style="@style/text_black_normal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@+id/onaPicha"
        android:hint="@string/jina"
        android:paddingBottom="3dp"
        android:paddingLeft="3dp"
        android:paddingRight="3dp"
        android:paddingTop="7dp"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/onaPicha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/onajina"
        android:padding="6dp"
        android:src="@drawable/icon" />

    <CheckBox
        android:id="@+id/checkBox1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusable="false"
        android:clickable="false"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/onajina"
        android:layout_marginRight="29dp" />

</com.example.regdemo.CheckableRelativeLayout>
